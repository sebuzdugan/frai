{
  "name": "framework-rai",
  "version": "1.0.3",
  "description": "Responsible AI framework for quick compliance documentation with AI-powered tips",
  "main": "ai-responsible.mjs",
  "bin": {
    "ai-responsible": "ai-responsible.mjs",
    "framework-rai": "ai-responsible.mjs"
  },
  "type": "module",
  "dependencies": {
    "dotenv": "^16.5.0",
    "inquirer": "^8.2.6",
    "node-fetch": "^3.3.2"
  },
  "devDependencies": {
    "tailwindcss": "^3.4.1",
    "autoprefixer": "^10.4.17",
    "postcss": "^8.4.35"
  },
  "keywords": [
    "ai",
    "responsible-ai",
    "ethics",
    "compliance",
    "documentation",
    "ai-governance",
    "model-cards"
  ],
  "author": "Sebastian Buzdugan",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sebastianbuzdugan/framework-rai.git"
  },
  "scripts": {
    "start": "node ai-responsible.mjs",
    "setup": "node ai-responsible.mjs --setup",
    "scan": "node ai-responsible.mjs --scan",
    "dev": "npx tailwindcss -i ./src/input.css -o ./public/styles.css --watch",
    "build": "npx tailwindcss -i ./src/input.css -o ./public/styles.css --minify"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}